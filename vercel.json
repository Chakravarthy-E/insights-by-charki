{
  "rewrites": [
    {
      "source": "collections/:category/:id",
      "destination": "collections/[category]/[id]"
    },
    {
      "source": "collections/:category",
      "destination": "collections/[category]"
    },
    {
      "source": "/about",
      "destination": "/about"
    },
    {
      "source": "/snippets",
      "destination": "/snippets"
    },
    {
      "source": "/collections",
      "destination": "/collections"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
