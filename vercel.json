{
  "rewrites": [
    {
      "source": "/:category/:slug",
      "destination": "/[category]/[slug]"
    },
    {
      "source": "collections/:category",
      "destination": "/collections/[category]"
    },
    {
      "source": "/collections",
      "destination": "/collections"
    },
    {
      "source": "/dsa",
      "destination": "/dsa"
    },
    {
      "source": "/dsa/collections",
      "destination": "/dsa/collections"
    },
    {
      "source": "/dsa/collections/:category",
      "destination": "/dsa/collections/[category]"
    },
    {
      "source": "/js-concepts",
      "destination": "/js-concepts"
    },
    {
      "source": "/js-concepts/collections",
      "destination": "/js-concepts/collections"
    },
    {
      "source": "/js-concepts/collections/:category",
      "destination": "/js-concepts/collections/[category]"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],

  "framework": "nextjs",
  "alias": ["insights-by-chakri"]
}
