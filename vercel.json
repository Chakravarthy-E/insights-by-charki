{
  "rewrites": [
    {
      "source": "/:category/:slug",
      "destination": "/[category]/[slug]"
    },
    {
      "source": "collections/:category",
      "destination": "/collections/[category]"
    },
    {
      "source": "/collections",
      "destination": "/collections"
    },
    {
      "source": "/dsa",
      "destination": "/dsa"
    },
    {
      "source": "/dsa/:category/:slug",
      "destination": "/dsa/[category]/[slug]"
    },
    {
      "source": "/dsa/collections",
      "destination": "/dsa/collections"
    },
    {
      "source": "/dsa/collections/:category",
      "destination": "/dsa/collections/[category]"
    },

    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],

  "framework": "nextjs"
}
